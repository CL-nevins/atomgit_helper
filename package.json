{
  "name": "git-emoji-atomgit",
  "displayName": "git-emoji-atomgit",
  "description": "git-emoji-atomgit 插件",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.91.0"
  },
  "author": {
    "name": "atomgit"
  },
  "license": "MIT",
  "categories": [
    "Other"
  ],
  "icon": "resources/logo.png",
  "preview": false,
  "keywords": [
    "git",
    "emoji",
    "atomgit"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "git-emoji-atomgit.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "git-emoji-atomgit.gitEmoji",
        "title": "git emoji: emoji list",
        "icon": {
          "dark": "resources/git-emoji.svg",
          "light": "resources/git-emoji_light.svg"
        }
      },
      {
        "command": "git-emoji-atomgit.formatSwitch",
        "title": "git emoji: format switch",
        "icon": {
          "dark": "resources/git-emoji.svg",
          "light": "resources/git-emoji_light.svg"
        }
      }
    ],
    "menus": {
      "scm/title": [
        {
          "when": "scmProvider == git",
          "command": "git-emoji-atomgit.gitEmoji",
          "alt": "git-emoji-atomgit.formatSwitch",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.91.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  },
  "packageManager": "pnpm@9.5.0+sha512.140036830124618d624a2187b50d04289d5a087f326c9edfc0ccd733d76c4f52c3a313d4fc148794a2a9d81553016004e6742e8cf850670268a7387fc220c903"
}
